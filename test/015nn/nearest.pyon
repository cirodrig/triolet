
def magnitude2(x): return dot(x, x)

def minIndex(xs):
    def pickMin(x, y):
    	(_, xv) = x
	(_, yv) = y
	return x if xv <= yv else y
    (i, _) = reduce1 (pickMin, zip(count, __iter__(xs)))
    return i

def nearestNeighbors(refs, inputs):
    return [minIndex(magnitude2(inp - ref) for ref in refs) for inp in inputs]

