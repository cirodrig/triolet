
def zips3(xs, ys, zs):
    return [(x - z) // (y - z) for (x, y, z) in zip3(xs, ys, zs)]

def zips4(xs, ys, zs, ws) -> list(int):
    return [(x + y) // (z - w) for (x, y, z, w) in zip4(xs, ys, zs, ws)]

export cplusplus zips3 : list(int) * list(int) * list(int) -> list(int)
export cplusplus zips4 : list(int) * list(int) * list(int) * list(int) -> list(int)

