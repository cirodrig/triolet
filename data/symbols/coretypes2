  (sh : bare) -> ShapeDict sh ->
  (a b : bare) ->
  Repr a -> Repr b -> (a -> Writer b) -> Stream sh a -> Stream sh b;

ShapeDict_Stream_zipWith :
  (sh : bare) -> ShapeDict sh ->
  (a b c : bare) ->
  Repr a -> Repr b -> Repr c ->
  (a -> b -> Writer c) ->
